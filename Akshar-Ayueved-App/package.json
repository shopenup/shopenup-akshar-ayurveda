    "@shopenup/shopenup-plugin-wishlist": "1.0.13",
    "@shopenup/types": "1.0.10",
    "@shopenup/ui": "^1.0.10",
    "@tanstack/react-query": "5.64.2",
    "@tiptap/extension-color": "^3.4.3",
    "@tiptap/extension-image": "^3.4.3",
